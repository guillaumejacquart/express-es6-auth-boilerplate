{
  "name": "express-es6-auth-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for ES2015 with local and facebook auth",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumejacquart/express-es6-auth-boilerplate.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guillaumejacquart/express-es6-auth-boilerplate/issues"
  },
  "homepage": "https://github.com/guillaumejacquart/express-es6-auth-boilerplate#readme",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-flash": "0.0.2",
    "express-handlebars": "^3.0.0",
    "express-nedb-session": "^0.1.1",
    "express-session": "^1.14.2",
    "nedb": "^1.8.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "spectre.css": "^0.1.31"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
}
